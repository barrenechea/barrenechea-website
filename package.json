    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@fontsource/work-sans": "^5.0.19",
    "@iconify-icons/circle-flags": "^1.2.41",
    "@tailwindcss/aspect-ratio": "^0.4.2",
